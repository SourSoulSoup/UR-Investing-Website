@model UR_Investing.Models.Member

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h3>@Html.DisplayFor(model => model.name)</h3>
    <h4>@Html.DisplayFor(model => model.Team.name) - @Html.DisplayFor(model => model.Position.name)</h4>
    <hr />
    <img class="bioPic" src="@Html.ValueFor(model => model.picturePath)" />
    @Html.DisplayFor(model => model.biography)
</div>

